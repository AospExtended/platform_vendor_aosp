<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="DocumentsTheme" parent="@android:style/Theme.DeviceDefault.DocumentsUI">
        <item name="android:colorBackground">@*android:color/primary_device_default_settings</item>
        <item name="android:windowBackground">?android:colorBackground</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowSoftInputMode">adjustUnspecified|stateUnspecified</item>
        <item name="android:actionModeBackground">?android:colorBackground</item>
        <item name="android:actionBarSize">48.0dip</item>
        <item name="android:strokeColor">#ff5f6368</item>
        <item name="android:colorControlActivated">?android:colorPrimary</item>
        <item name="android:colorPrimary">@*android:color/accent_device_default_dark</item>
        <item name="android:colorAccent">@*android:color/accent_device_default_dark</item>
        <item name="android:statusBarColor">?android:colorBackground</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="android:colorBackgroundFloating">#ff3c4043</item>
        <item name="android:colorSecondary">#3d8ab4f8</item>
        <item name="android:windowLightNavigationBar">false</item>
    </style>
</resources>
